instances = []
movables = []

class Obj:
  def __init__(self, *arg):
    self.xPos = arg[0]
    self.yPos = arg[1]
    self.xSpd = arg[2]
    self.ySpd = arg[3]
    if arg[4]:
      movables.append(self)


for i in range(5):
  instances.append(Obj(0,0,1,2,1))


def move(movables):
  for i in movables:
    print("moved "+str((i.xPos, i.yPos, i.xSpd, i.ySpd)))
    i.xPos += i.xSpd
    i.ySpd += i.ySpd

move(movables)
